package edu.fcla.daitss.file;

/**
 * TransformationFormat represents (usually complex) formats
 * into which formats can be transformed.
 */
public class TransformationFormat {
	/**
     * The JPEG_JP2_1 format
     */
    public static final String JPEG_JP2_1 = "JPEG_JP2_1";
	
    /**
     * The PDF_NORM_1 format
     */
    public static final String PDF_NORM_1 = "PDF_NORM_1";

    /**
     * Normalizaton format for video and audio streams in AVI files 
     */
    public static final String AVI_NORM_1 = "AVI_NORM_1";
    
    /**
     * Normalizaton format for video and audio streams in QuickTime files 
     */
    public static final String QUICKTIME_NORM_1 = "QUICKTIME_NORM_1";
    
    /**
     * Normalizaton format for the audio stream in wave files
     */
    public static final String WAVE_NORM_1 = "WAVE_NORM_1";
}
