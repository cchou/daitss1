package edu.fcla.daitss.prep.xmission;

import java.io.File;
import java.io.IOException;

/**
 * An interface for a class that unpacks many files from one packed file.
 * 
 * @author franco
 * 
 */
public interface Unpacker {
	/**
	 * @param pkg
	 * @return a directory containing the unpacked files
	 * @throws IOException
	 */
	public File unpack(File pkg) throws IOException;
}
